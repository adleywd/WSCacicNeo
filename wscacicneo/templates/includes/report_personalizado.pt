<div class="matter">
   <div class="container">
      <div class="row">
         <div class="col-md-12">
            <div tal:repeat="RD report_data" class="widget" id="tabela_relatorio">
               <div class="widget-head">
                  <div class="pull-left">Relatório ${RD[0]}</div>
                  <div class="clearfix"></div>
               </div>
               <div class="widget-content">
                  <div id="error-message" class="alert alert-warning" style="display:none">
                     Operação realizada com erro!
                  </div>
                  <div id="success-message" class="alert alert-success"  style="display:none">
                     Operação realizada com sucesso!
                  </div>
                  <div class="padd">
                     <div class="page-tables">
                        <div class="table-responsive ">
                           <table class="table table-condensed table-bordered table-hover no-margin">
                              <thead>
                                 <tr class="info">
                                    <th>Item</th>
                                    <th>Quantidade</th>
                                 </tr>
                              </thead>
                              <tbody>
                                 <tr class="active" tal:repeat="k RD[1].keys()">
                                    <td>${k}</td>
                                    <td data-type="text">${RD[1][k]}</td>
                                 </tr>
                              </tbody>
                           </table>
                           <div class="clearfix"></div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <div class="box-chart">
         <canvas id="GraficoLine" style="width:100%;"></canvas>
      </div>
   </div>
</div>
