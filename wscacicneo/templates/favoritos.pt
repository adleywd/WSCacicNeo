<metal:main use-macro="load: master.pt">
	<metal:content fill-slot="conteudo">
        <div class="col-md-4" style="margin-left: 200px;">
              <div class="widget">
                <div class="widget-head">
                  <div class="pull-left">Editar</div>
                  <div class="widget-icons pull-right">
                    <a href="#" class="wminimize"><i class="fa fa-chevron-up"></i></a> 
                  </div>
                  <div class="clearfix"></div>
                </div>
                <div class="widget-content">
                  <div class="padd">
                    <!-- Visitors, pageview, bounce rate, etc., Sparklines plugin used -->
                    <ul id='favoritos-off' class="current-status">
                        <label class="checkbox-inline">
                            <input type="checkbox" id="cadastro">Cadastro de Órgãos
                        </label>
                      <li>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="inlineCheckbox3">Lista de Órgãos
                        </label>
                      </li>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="inlineCheckbox3">Cadastro de Usuario
                        </label>
                      <li>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="inlineCheckbox3">Lista de Usuario
                        </label>
                      </li>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="inlineCheckbox3">Gráficos
                        </label>
                      <li>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="inlineCheckbox3">Questionar Coleta
                        </label>
                      </li>
                        <label class="checkbox-inline">
                            <input type="checkbox" id="inlineCheckbox3">Notificações
                        </label>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-4">
              <div class="widget">
                <div class="widget-head">
                  <div class="pull-left">Favoritos</div>
                  <div class="widget-icons pull-right">
                    <a href="#" class="wminimize"><i class="fa fa-chevron-up"></i></a>
                  </div>
                  <div class="clearfix"></div>
                </div>
                <div class="widget-content">
                  <div class="padd">
                    <!-- Visitors, pageview, bounce rate, etc., Sparklines plugin used -->
                    <ul tal:repeat="doc favoritos" id="favoritos-on" class="current-status">
                        <li>
                            <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox3" value="option3">${doc}
                            </label>
                        </li>
                    </ul>
                  </div>
                </div>
              </div>
            <button type="button" class="btn btn-sm btn-default">Salvar</button>
            </div>
    </metal:content>
	<metal:content fill-slot="javascript">
        <script type="text/javascript">
        </script>
    </metal:content>
</metal:main>
