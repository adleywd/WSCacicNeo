<metal:main use-macro="load: master.pt">
	<metal:content fill-slot="conteudo">
        <div class="col-md-4" style="margin-left: 200px;">
              <div class="widget">
                <div class="widget-head">
                  <div class="pull-left">Editar</div>
                  <div class="widget-icons pull-right">
                    <a href="#" class="wminimize"><i class="fa fa-chevron-up"></i></a> 
                  </div>
                  <div class="clearfix"></div>
                </div>
                <div class="widget-content">
                  <div class="padd">
                    <!-- Visitors, pageview, bounce rate, etc., Sparklines plugin used -->
                    <ul id='favoritos-off' class="current-status">
                      <li class='cadastro-o'>
                        <span id='cadastroorgao'>Cadastro Órgãos</span>
                      </li>
                        <span id="listaorgaos">Lista Órgãos</span>
                      <li>
                        <span id='cadastrousuario'>Cadastro Usuarios</span>
                      </li>
                        <span class='listausuario'>Lista de Usuarios</span>
                      <li>
                        <span class='relatorios'>Relatorios</span>
                      </li>
                        <span class='graficos'>Graficos</span>
                      <li>
                        <span class='coleta'>Questionar Coleta</span>
                      </li>
                        <span class='notify'>Notificações</span>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-4">
              <div class="widget">
                <div class="widget-head">
                  <div class="pull-left">Favoritos</div>
                  <div class="widget-icons pull-right">
                    <a href="#" class="wminimize"><i class="fa fa-chevron-up"></i></a>
                  </div>
                  <div class="clearfix"></div>
                </div>
                <div class="widget-content">
                  <div class="padd">
                    <!-- Visitors, pageview, bounce rate, etc., Sparklines plugin used -->
                    <ul  tal:repeat="doc favoritos" id="favoritos-on" class="current-status">
                        <li>
                            <span>${doc}</span>
                        </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
    </metal:content>
	<metal:content fill-slot="javascript">
        <script type="text/javascript">
            $('span').click(function(){
            });
        </script>
    </metal:content>
</metal:main>
